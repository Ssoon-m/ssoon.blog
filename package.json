{
  "name": "ianlog.me",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "yarn upload-cloudinary && next build && yarn generate-sitemap",
    "generate-sitemap": "ts-node --project tsconfig.node.json ./scripts/generate-sitemap.ts",
    "upload-cloudinary": "ts-node --project tsconfig.node.json ./scripts/upload-cloudinary.ts",
    "start": "npx serve@latest out",
    "prepare": "husky install",
    "lint-fix:prettier": "prettier --write \"./**/*.{js,ts,tsx}\" --ignore-path .gitignore --ignore-path .prettierignore ",
    "lint-fix": "next lint --fix",
    "ts-check": "tsc --noEmit",
    "lint": "next lint",
    "run-lint-staged": "lint-staged"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "framer-motion": "^10.16.4",
    "next": "13.4.19",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.6.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "cloudinary": "^1.41.0",
    "contentlayer": "^0.3.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "github-slugger": "^2.0.0",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "next-contentlayer": "^0.3.4",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-prism-plus": "^1.6.3",
    "rehype-slug": "^6.0.0",
    "remark-breaks": "^3.0.3",
    "remark-gfm": "^3.0.1",
    "schema-dts": "^1.1.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@3.6.2"
}
